<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @physician.name %>
</p>

<p>
  <strong>Specialty:</strong>
  <%= @physician.specialty %>
</p>

<h3>Office Locations</h3>
  <table class="blueTable">
    <tr>
      <th>Street Address</th> <th>Zip Code</th> <th>Office Phone</th>
    </tr>

    <tr>
      <% @physician.offices.each do |office|%>

      <td><%= office.street_address%></td> <td><%= office.zip_code%></td> <td><%= office.phone%></td>

      <% end %>

    </tr>
  
  </table>
    

<%= link_to 'Edit', edit_physician_path(@physician) %> |
<%= link_to 'Back', physicians_path %>
